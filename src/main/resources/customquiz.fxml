<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #BDBDBD;"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.controllers.startup.CustomQuizController">
    <children>
        <TextField id="questionAmountTextField" fx:id="questionAmountField" layoutX="412.0" layoutY="198.0"
                   prefHeight="26.0" prefWidth="63.0"/>
        <Button id="startButton" fx:id="startButton" layoutX="504.0" layoutY="361.0" mnemonicParsing="false"
                onMouseClicked="#onStartButton" text="Start Quiz"/>
        <ChoiceBox fx:id="subjectList" layoutX="84.0" layoutY="237.0" prefHeight="26.0" prefWidth="117.0"/>
        <Label layoutX="21.0" layoutY="237.0" prefHeight="26.0" prefWidth="63.0" text="Subject:"/>
        <Label layoutX="21.0" layoutY="270.0" prefHeight="26.0" prefWidth="50.0" text="Type:"/>
        <ChoiceBox fx:id="typeList" layoutX="84.0" layoutY="270.0" prefHeight="26.0" prefWidth="117.0"/>
        <Label layoutX="332.0" layoutY="115.0" prefHeight="26.0" prefWidth="86.0" text="Test Name:"/>
        <TextField fx:id="testNameField" layoutX="411.0" layoutY="115.0" prefHeight="26.0" prefWidth="150.0"/>
        <Label layoutX="332.0" layoutY="198.0" prefHeight="26.0" prefWidth="86.0" text="Question #"/>
        <Label fx:id="test" layoutX="332.0" layoutY="156.0" prefHeight="26.0" prefWidth="80.0" text="Test Time:"/>
        <TextField fx:id="testTimeField" layoutX="411.0" layoutY="156.0" prefHeight="26.0" prefWidth="150.0"
                   promptText="Value in minutes"/>
        <Button fx:id="backButton" layoutX="450.0" layoutY="361.0" mnemonicParsing="false" onAction="#onBackButton"
                text="Back"/>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#829ab1" height="18.0" layoutX="15.0" layoutY="90.0"
                   stroke="WHITE" strokeType="INSIDE" strokeWidth="0.0" width="200.0"/>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#c32148" height="11.0" layoutX="7.0" layoutY="46.0"
                   stroke="BLACK" strokeLineCap="ROUND" strokeType="INSIDE" strokeWidth="0.0" width="586.0"/>
        <Text fill="#c32148" layoutX="21.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Custom Options"
              wrappingWidth="579.7294921875">
            <font>
                <Font size="26.0"/>
            </font>
        </Text>
        <VBox layoutX="15.0" layoutY="115.0" prefHeight="81.0" prefWidth="200.0">
            <children>
                <CheckBox fx:id="calcPref" mnemonicParsing="false" text="Calculator"/>
                <CheckBox fx:id="drawPref" mnemonicParsing="false" text="Drawing Pad"/>
                <CheckBox fx:id="notePadPref" mnemonicParsing="false" prefHeight="18.0" prefWidth="91.0"
                          text="Notepad"/>
                <CheckBox fx:id="correctAnswers" mnemonicParsing="false" text="Show Correct Answers"/>
            </children>
        </VBox>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#829ab1" height="18.0" layoutX="15.0" layoutY="206.0"
                   stroke="WHITE" strokeType="INSIDE" strokeWidth="0.0" width="200.0"/>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#829ab1" height="18.0" layoutX="335.0" layoutY="90.0"
                   stroke="WHITE" strokeType="INSIDE" strokeWidth="0.0" width="230.0"/>
    </children>
</AnchorPane>
