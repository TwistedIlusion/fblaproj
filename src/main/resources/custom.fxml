<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #BDBDBD;"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.controllers.CustomTestController">
    <children>
        <TextField id="questionAmountTextField" fx:id="questionAmountField" layoutX="108.0" layoutY="246.0"
                   prefHeight="26.0" prefWidth="150.0"/>
        <Button id="startButton" fx:id="startButton" layoutX="536.0" layoutY="361.0" mnemonicParsing="false"
                onMouseClicked="#onStartButton" text="Start"/>
        <ChoiceBox fx:id="subjectList" layoutX="108.0" layoutY="56.0" prefHeight="26.0" prefWidth="150.0"/>
        <Label layoutX="30.0" layoutY="58.0" prefHeight="26.0" prefWidth="63.0" text="Subject:"/>
        <Label layoutX="30.0" layoutY="154.0" prefHeight="26.0" prefWidth="50.0" text="Type:"/>
        <ChoiceBox fx:id="typeList" layoutX="108.0" layoutY="154.0" prefWidth="150.0"/>
        <Label layoutX="19.0" layoutY="319.0" prefHeight="26.0" prefWidth="86.0" text="Test Name:"/>
        <CheckBox fx:id="calcPref" layoutX="300.0" layoutY="60.0" mnemonicParsing="false" text="Calculator"/>
        <CheckBox fx:id="drawPref" layoutX="300.0" layoutY="84.0" mnemonicParsing="false" text="Drawing Pad"/>
        <CheckBox fx:id="notePadPref" layoutX="300.0" layoutY="110.0" mnemonicParsing="false" text="Notepad"/>
        <TextField fx:id="testNameField" layoutX="108.0" layoutY="319.0" prefHeight="26.0" prefWidth="150.0"/>
        <Label layoutX="19.0" layoutY="246.0" prefHeight="26.0" prefWidth="86.0" text="Question #"/>
    </children>
</AnchorPane>
