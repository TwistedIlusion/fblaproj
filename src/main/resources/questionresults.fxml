                <Button fx:id="backButton" layoutX="892.0" layoutY="112.0" mnemonicParsing="false"
                        onAction="#onBackButton" prefHeight="26.0" prefWidth="142.0" text="Back to Results"
                        AnchorPane.leftAnchor="892.0" AnchorPane.rightAnchor="892.0"/>
