<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.alert.quizkey.EnterQuizKeyScreen">
   <children>
      <VBox alignment="CENTER" layoutX="4.0" layoutY="7.0" prefHeight="104.0" prefWidth="202.0" spacing="20.0"
            stylesheets="@../css/textfield.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Enter a Test Key">
               <font>
                  <Font size="15.0"/>
               </font>
            </Label>
            <TextField fx:id="codeTextField" prefHeight="26.0" prefWidth="146.0"/>
            <HBox minWidth="200.0" spacing="15.0" stylesheets="@../css/button.css">
               <children>
                  <Button mnemonicParsing="false" onAction="#enterCodeButtonClicked" text="Enter Key"/>
                  <Button mnemonicParsing="false" onAction="#exitButtonClicked" prefHeight="22.0" prefWidth="81.0"
                          text="Exit"/>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
         </padding>
      </VBox>
   </children>
</AnchorPane>
