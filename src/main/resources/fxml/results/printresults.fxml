                     <image>
                        <Image url="@../../pictures/menu/FBLA-Color-web.png"/>
                     </image>
                  </ImageView>
               </children>
            </AnchorPane>
            <HBox alignment="CENTER_LEFT"
                  style="-fx-border-style: hidden hidden solid hidden; -fx-border-color: #303f9f; -fx-border-width: 3; -fx-border-radius: 1;">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0"/>
               </VBox.margin>
               <children>
                  <Label fx:id="quizName1" text="Results" textFill="#303f9f">
                     <font>
                        <Font name="SansSerif Regular" size="30.0"/>
                     </font>
                  </Label>
                  <Region HBox.hgrow="ALWAYS"/>
               </children>
            </HBox>
            <VBox alignment="CENTER">
               <VBox.margin>
                  <Insets top="50.0"/>
               </VBox.margin>
               <children>
                  <Label fx:id="outOfLabel" text="0 out of 0" textFill="#303f9f">
                     <font>
                        <Font size="30.0"/>
                     </font>
                  </Label>
                  <Label fx:id="percentageLabel" text="0%" textFill="#303f9f">
                     <font>
                        <Font size="30.0"/>
                     </font>
                  </Label>
               </children>
            </VBox>
            <VBox alignment="CENTER">
               <children>
                  <Label text="Enter Name" textFill="#303f9f">
                     <font>
                        <Font size="20.0"/>
                     </font>
                  </Label>
                  <HBox alignment="CENTER">
                     <children>
                        <TextField/>
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets top="35.0"/>
               </VBox.margin>
            </VBox>
            <Region VBox.vgrow="ALWAYS"/>
            <HBox alignment="CENTER">
               <children>
                  <ScrollPane style="-fx-background-color: #E1E1E1;">
                     <content>
                        <VBox fx:id="questionsVBox" alignment="CENTER" spacing="30.0"
                              style="-fx-background-color: #E1E1E1;"/>
                     </content>
                  </ScrollPane>
               </children>
               <VBox.margin>
                  <Insets left="15.0" right="15.0" top="30.0"/>
               </VBox.margin>
            </HBox>
            <AnchorPane fx:id="buttonPane1" style="-fx-background-color: #9E9E9E;">
               <VBox.margin>
                  <Insets top="150.0"/>
               </VBox.margin>
               <children>
                  <HBox alignment="CENTER" fillHeight="false" layoutX="14.0" layoutY="6.0" spacing="10.0"
                        stylesheets="@../css/button.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                     </padding>
                     <children>
                        <Button fx:id="screenshotButton" mnemonicParsing="false" onAction="#screenshotButtonClicked"
                                text="Screenshot"/>
                        <Button fx:id="printButton1" mnemonicParsing="false" onAction="#printButtonClicked"
                                text="Print"/>
                        <Button mnemonicParsing="false" onAction="#backToMainMenuClicked" text="classes.Main Menu"/>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
