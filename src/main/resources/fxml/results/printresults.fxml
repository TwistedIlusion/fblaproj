<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="mainAnchor1" style="-fx-background-color: #E1E1E1;" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="classes.gui.quiz.results.PrintResults">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
            AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane fx:id="topPane1" style="-fx-background-color: #9E9E9E;">
               <children>
                  <HBox alignment="CENTER_RIGHT" layoutX="828.0" layoutY="14.0" spacing="10.0"
                        AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="15.0">
                     <padding>
                        <Insets right="15.0"/>
                     </padding>
                     <children>
                        <ImageView fitHeight="25.0" fitWidth="25.0">
                           <image>
                              <Image url="@../../pictures/menu/user-circle-solid.png"/>
                           </image>
                        </ImageView>
                        <Label fx:id="userLabel1" alignment="CENTER_RIGHT" textFill="#c32148">
                           <font>
                              <Font size="18.0"/>
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <ImageView fitHeight="43.0" fitWidth="60.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0"
