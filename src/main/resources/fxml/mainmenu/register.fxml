<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="122.0" prefWidth="261.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.mainmenu.login.Register">
   <children>
      <VBox layoutX="8.0" layoutY="8.0" prefHeight="122.0" prefWidth="273.0" spacing="10.0">
         <children>
            <HBox prefHeight="26.0" prefWidth="258.0" spacing="15.0">
               <children>
                  <Label prefHeight="27.0" prefWidth="67.0" text="Username"/>
                  <TextField fx:id="usernameField"/>
               </children>
            </HBox>
            <HBox prefHeight="26.0" prefWidth="258.0" spacing="16.0">
               <children>
                  <Label prefHeight="31.0" prefWidth="67.0" text="Password"/>
                  <TextField fx:id="passwordField"/>
               </children>
            </HBox>
            <Label fx:id="errorLabel" prefHeight="16.0" prefWidth="285.0" textFill="RED"/>
            <HBox alignment="CENTER" prefHeight="26.0" prefWidth="171.0" spacing="10.0"
                  stylesheets="@../css/standard_button.css">
               <children>
                  <Button fx:id="loginButton" mnemonicParsing="false" onAction="#registerButtonClicked"
                          prefHeight="22.0" prefWidth="94.0" text="Register"/>
                  <Hyperlink onAction="#loginButtonClicked" text="Login to Existing"/>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0"/>
   </padding>
</AnchorPane>
