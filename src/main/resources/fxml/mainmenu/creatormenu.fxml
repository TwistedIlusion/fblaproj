<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="740.0" prefWidth="1000.0" stylesheets="@../css/general.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.benschreiber.gui.windows.mainmenu.CreatorMenu">
    <children>
        <HBox layoutX="398.0" layoutY="81.0" spacing="15.0">
            <children>
                <Button mnemonicParsing="false" onAction="#uploadQuizClicked" text="Upload New Quiz" />
                <Button mnemonicParsing="false" onAction="#quizBuilderClicked" text="Quiz Builder" />
                <Button mnemonicParsing="false" onAction="#deleteQuizClicked" text="Delete Quiz" />
                <Button mnemonicParsing="false" onAction="#getExcelClicked" text="Get Excel" />
            </children>
        </HBox>
        <TableView fx:id="quizzesTable" layoutX="75.0" layoutY="107.0" maxWidth="856.0" prefHeight="349.0" prefWidth="850.0" stylesheets="@../css/tableview.css">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="-1.0" text="Quiz" />
                <TableColumn fx:id="keyColumn" minWidth="-1.0" prefWidth="-1.0" text="Key" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <Label layoutX="75.0" layoutY="82.0" maxWidth="1.7976931348623157E308" text="Created Quizzes">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
