<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.quiz.notepad.NotePadController">
    <children>
        <TextArea fx:id="notepadText" prefHeight="363.0" prefWidth="600.0" promptText="Enter some notes..."/>
        <Button fx:id="saveButton" layoutX="536.0" layoutY="363.0" mnemonicParsing="false"
                onMouseClicked="#onSaveButton" text="Save"/>
    </children>
</AnchorPane>
