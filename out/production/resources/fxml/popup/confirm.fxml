<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.benschreiber.gui.alert.confirm.ConfirmScreenController">
   <children>
      <VBox alignment="TOP_CENTER" layoutY="3.0" spacing="10.0" AnchorPane.bottomAnchor="0.0"
            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="promptLabel" alignment="CENTER"
                   text="aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa">
               <padding>
                  <Insets top="10.0"/>
               </padding>
            </Label>
            <HBox alignment="CENTER" prefHeight="41.0" prefWidth="200.0" spacing="10.0"
                  stylesheets="@../css/button.css">
               <children>
                  <Button mnemonicParsing="false" onAction="#confirmButtonClicked" text="Confirm"/>
                  <Button mnemonicParsing="false" onAction="#denyButtonClicked" prefHeight="22.0" prefWidth="86.0"
                          text="Deny"/>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0"/>
         </padding>
      </VBox>
   </children>
</AnchorPane>
