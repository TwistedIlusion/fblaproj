<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.benschreiber.gui.windows.alert.notification.NotificationScreenController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="11.0" layoutY="4.0" spacing="30.0" AnchorPane.bottomAnchor="0.0"
            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="errorLabel" text="error"/>
            <Button mnemonicParsing="false" onAction="#buttonClicked" stylesheets="@../css/button.css"
                    text="OK"/>
         </children>
         <padding>
            <Insets bottom="15.0" left="50.0" right="50.0" top="15.0"/>
         </padding>
      </VBox>
   </children>
</AnchorPane>
